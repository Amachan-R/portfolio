<h2>プロフィール編集</h2>
<%= form_with model:@user, local:true do |f| %>
  <%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %>
  <h4>プロフィール画像</h4>
  <%= f.attachment_field :profile_image %>
  <h4>ニックネーム</h4>
  <%= f.text_field :name %>
  <h4>自己紹介</h4>
  <%= f.text_area :introduction %>

  <%= f.submit '変更を保存' %>
<% end %>